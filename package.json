{
  "name": "airborne",
  "version": "0.12.0",
  "description": "Airborne RESTful API framework",
  "main": "dist/index.js",
  "scripts": {
    "precommit": "echo hello",
    "check-coverage": "nyc check-coverage --statements 80 --branches 80 --functions 80 --lines 80",
    "report-coverage": "cat ./coverage/lcov.info | codecov",
    "test": "mocha test --recursive --compilers js:babel-register",
    "test:watch": "npm t -- -w",
    "cover": "nyc npm t",
    "lint": "eslint src",
    "build:watch": "eslint src && babel -w src -d dist",
    "prebuild": "rimraf dist",
    "build": "eslint src && babel src -d dist"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/niklucky/airborne.git"
  },
  "keywords": [
    "ariborne",
    "engine",
    "framework",
    "api",
    "dao",
    "mapper",
    "controller",
    "service",
    "model",
    "schema",
    "mysql",
    "redis",
    "database"
  ],
  "files": [
    "dist",
    "docs",
    "examples",
    "README.md"
  ],
  "author": "Nikita Zhuravlev",
  "license": "MIT",
  "homepage": "https://github.com/niklucky/airborne#readme",
  "devDependencies": {
    "babel-cli": "^6.11.4",
    "babel-core": "^6.11.4",
    "babel-eslint": "^6.1.2",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-0": "^6.5.0",
    "babel-register": "^6.11.6",
    "chai": "^3.5.0",
    "chai-as-promised": "^6.0.0",
    "codecov.io": "^0.1.6",
    "eslint": "^3.7.1",
    "eslint-config-airbnb": "^12.0.0",
    "eslint-plugin-import": "^1.16.0",
    "eslint-plugin-jsx-a11y": "^2.2.2",
    "eslint-plugin-promise": "^2.0.1",
    "eslint-plugin-react": "^6.3.0",
    "eslint-plugin-standard": "^2.0.1",
    "husky": "^0.13.1",
    "istanbul": "^0.4.5",
    "mocha": "^2.5.3",
    "nyc": "^8.4.0",
    "rimraf": "^2.5.4"
  },
  "dependencies": {
    "airborne-base": "^0.6.0",
    "body-parser": "^1.15.2",
    "chai-http": "^3.0.0",
    "express": "^4.14.0",
    "formidable": "^1.1.1",
    "mysql": "^2.15.0",
    "mysql-qb": "^0.8.1",
    "redis": "^2.8.0",
    "sanitizer": "^0.1.3",
    "superagent": "^3.8.2"
  },
  "engines": {
    "node": ">=4.1.2 =<6.3"
  }
}
